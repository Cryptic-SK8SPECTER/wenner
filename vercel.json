{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "NODE_ENV": "production",
      "PYTHON": "/usr/bin/python3",
      "NODE_OPTIONS": "--max-old-space-size=4096"
    }
  }
  